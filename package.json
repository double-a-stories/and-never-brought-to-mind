{
  "name": "life-of-the-party",
  "version": "0.5.0-indev",
  "description": "A furry vore adventure",
  "scripts": {
    "twee-build": "tweego -m src/modules/ --head=src/head-content.html -o dist/index.html src/twee",
    "twee-watch": "npm run twee-build -- -w",
    "serve": "concurrently 'npm run twee-watch' 'live-server --watch=dist --open=dist'",
    "build-proof": "tweego -f poof -m src/modules/ --head=src/head-content.html -o proofing/poof.html src/twee; tweego -f illume -m src/modules/ --head=src/head-content.html -o proofing/illume.html src/twee; tweego -f dotgraph -m src/modules/ --head=src/head-content.html -o proofing/dotgraph.html src/twee;",
    "deploy-stable": "gh-pages -a -d dist",
    "deploy-indev": "gh-pages -a -d dist -e indev/"
  },
  "devDependencies": {
    "concurrently": "^6.1.0",
    "gh-pages": "^3.1.0",
    "http-live-server": "^1.2.0",
    "opn-cli": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/double-a-stories/life-of-the-party.git"
  },
  "author": "Double-A",
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/double-a-stories/life-of-the-party/issues"
  },
  "homepage": "https://github.com/double-a-stories/life-of-the-party#readme"
}
