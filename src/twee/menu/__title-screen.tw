    %>
    <li>
    <a class="<%= !unlocked ? "disabled" : "" %>" href="javascript:void(0)" data-passage="<%= name %>"><%= name %></a>
    </li>
    <%
    }
  %>
</ul>

<div class="content-warning">
<%= story.render("Content Warning") %>
</div>

