:: Links [script]

$(window).on('sm.passage.shown', () => {
  $("tw-passage ul a[href='javascript:void(0)'], .command-link").each((i, el) => {
    $(el).unwrap("p");
    $(el).parents("ul").addClass("commands-list");
  })
});

:: Links styles [stylesheet]

.commands-list {
  list-style: "> " inside;
  padding-inline-start: unset;
}

.commands-list li {
  margin-bottom: 1rem;
}