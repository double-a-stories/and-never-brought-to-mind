{
  "name": "life-of-the-party",
  "version": "0.5.0-indev",
  "description": "A furry vore adventure",
  "scripts": {
    "build": "tweego -m src/modules/ --head=src/head-content.html -o dist/index.html src/twee",
    "watch": "opn dist/index.html; tweego -w -m src/modules/ --head=src/head-content.html -o dist/index.html src/twee/",
    "deploy": "gh-pages -a -d dist",
    "deploy-indev": "gh-pages -a -d dist -e indev/"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "opn-cli": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/double-a-stories/life-of-the-party.git"
  },
